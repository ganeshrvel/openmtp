{
  "name": "openmtp",
  "productName": "OpenMTP",
  "version": "3.2.10",
  "description": "OpenMTP | Android File Transfer for macOS",
  "scripts": {
    "build": "yarn lint && concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-no-verify": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./webpack/config.renderer.dev.dll.babel.js --color",
    "build-main": "cross-env NODE_ENV=production webpack --config ./webpack/config.main.prod.babel.js --color",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./webpack/config.renderer.prod.babel.js --color",
    "dev": "cross-env NODE_ENV=development START_HOT=1 node -r @babel/register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start-renderer-dev",
    "dev-kalam-ffi": "./ffi/kalam/native/scripts/build.sh && yarn dev",
    "package": "yarn build && electron-builder --config electron-builder-config.js build --publish never",
    "package-all": "yarn build && electron-builder --config electron-builder-config.js build -mwl --publish never",
    "package-mac": "yarn build && electron-builder --config electron-builder-config.js build --mac --publish never",
    "package-mac-no-verify": "yarn build-no-verify && electron-builder --config electron-builder-config.js build --mac --publish never",
    "package-mac-without-notarize": "yarn build && cross-env ELECTRON_NOTARIZE=NO electron-builder --config electron-builder-config.js build --mac --publish never",
    "package-mac-without-notarize-no-verify": "yarn build-no-verify && cross-env ELECTRON_NOTARIZE=NO electron-builder --config electron-builder-config.js build --mac --publish never",
    "package-linux": "yarn build && electron-builder --config electron-builder-config.js build --linux --publish never",
    "package-win": "yarn build && electron-builder --config electron-builder-config.js build --win --x64 --publish never",
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production ./app/main.prod.js",
    "start-main-dev": "cross-env START_HOT=1 NODE_ENV=development run-electron -r @babel/register ./app/main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack serve --config webpack/config.renderer.dev.babel.js",
    "publish-mac": "yarn build && electron-builder --config electron-builder-config.js build --mac --publish always",
    "publish-mac-no-verify": "yarn build-no-verify && cross-env electron-builder --config electron-builder-config.js build --mac --publish always",
    "dev-docs": "cross-env NODE_ENV=development webpack serve --config docs-sources/webpack/webpack.config.base.js",
    "build-docs": "cross-env NODE_ENV=production webpack --config docs-sources/webpack/webpack.config.base.js && git add .",
    "lint": "cross-env NODE_ENV=development eslint --cache . --format=pretty --ext .js,.jsx",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --custom-syntax postcss-scss",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "lint-styles-fix": "yarn --silent lint-styles --fix; exit 0",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.*{js,jsx,json}'",
    "postlint-styles-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{css,scss}'",
    "preinstall": "internals/scripts/preinstall.sh",
    "postinstall": "yarn build-dll && electron-builder install-app-deps"
  },
  "main": "./app/main.prod.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshrvel/openmtp.git"
  },
  "author": {
    "name": "Ganesh Rathinavel",
